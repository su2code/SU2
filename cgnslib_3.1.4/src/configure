            if test -f "$d/libsz.$a"; then
              SZIPLIB=`(cd $d; pwd)`/libsz.$a
              echo "$ac_t""$SZIPLIB" 1>&6
              break
            fi
          done
          test ! -z "$SZIPLIB" && break
        done
      fi
    else
      echo "$ac_t""$sziplib" 1>&6
      SZIPLIB=$sziplib
    fi
  fi
fi

